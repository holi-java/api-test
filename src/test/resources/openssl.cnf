prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions     = req_ext

[ dn ]

C=CN
ST=Guang Zhou
L=Guang Zhou
O=holi-java
OU=holi-java
emailAddress=holi-java@foxmail.com
CN = localhost

[ req_ext ]

basicConstraints = CA:FALSE
